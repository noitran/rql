{
    "name": "noitran/rql",
    "description": "RQL - Resource Query Language package for laravel / lumen projects",
    "authors": [
        {
            "name": "Noitran.Black",
            "email": "noitran.black@gmail.com"
        }
    ],
    "keywords": [
        "laravel",
        "lumen",
        "api",
        "eloquent",
        "model",
        "mysql",
        "mongodb",
        "microservice",
        "rql",
        "resource-query-language"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "illuminate/database": "~5.8",
        "illuminate/support": "~5.8"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "orchestra/testbench": "~3.8.0",
        "nesbot/carbon": "^1.26.3"
    },
    "autoload": {
        "psr-4": {
            "Noitran\\RQL\\": "src"
        },
        "files": [
            "helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Noitran\\RQL\\Tests\\": "tests"
        },
        "files": [
            "helpers.php"
        ]
    }
}
